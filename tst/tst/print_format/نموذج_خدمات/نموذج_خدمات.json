 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"UTF-8\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n    <title>\u0646\u0645\u0648\u0630\u062c \u062e\u062f\u0645\u0627\u062a</title>\n    <link\n      href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n      rel=\"stylesheet\"\n      integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n      crossorigin=\"anonymous\"\n    />\n    <style>\n      .print-format {\n        padding: 0px !important;\n        font-size: 16px !important;\n      }\n      table {\n        font-size: 26px !important;\n        font-weight: bold !important;\n      }\n\n      table tr th {\n        font-weight: bold !important;\n        color: #000 !important;\n      }\n\n      table tr th,\n      table tr td {\n        border: 1px solid #000 !important;\n        padding: 10px !important;\n      }\n    </style>\n  </head>\n  <body>\n    <link\n      href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css\"\n      rel=\"stylesheet\"\n      integrity=\"sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC\"\n      crossorigin=\"anonymous\"\n    />\n    <main>\n      <header>\n        <img\n          src=\"/assets/tst/images/quotation_cover_3.jpeg\"\n          alt=\"logo\"\n          width=\"100%\"\n        />\n      </header>\n      <div class=\"page-break\"></div>\n      <section style=\"padding: 20px 20px 0 0\">\n        <img src=\"/assets/tst/images/quotation_proposal.jpeg\" width=\"100%\" />\n        <div\n          style=\"\n            border: 1px solid #ffc000;\n            background-color: #ffc000;\n            padding: 10px;\n            display: flex;\n            justify-content: space-between;\n          \"\n        >\n          <div>\n            <h3 style=\"font-weight: bold\">Customer Name: {{doc.customer_name or doc.customer}}</h3>\n            <span> </span>\n          </div>\n        </div>\n        <div\n          style=\"\n            border: 1px solid #ffc000;\n            padding: 10px;\n            display: flex;\n            justify-content: space-between;\n          \"\n        >\n          <div>\n            <h3 style=\"font-weight: bold\">Contact Person: {{doc.contact_display or \"\"}}</h3>\n            <span> </span>\n          </div>\n          <h3 style=\"font-weight: bold\"></h3>\n        </div>\n        <div\n          style=\"\n            border: 1px solid #ffc000;\n            background-color: #ffc000;\n            padding: 10px;\n            display: flex;\n            justify-content: space-between;\n          \"\n        >\n          <div>\n            <h3 style=\"font-weight: bold\">Mobile Number: {{doc.contact_mobile or \"\"}}</h3>\n            \n          </div>\n          <h3 style=\"font-weight: bold\"></h3>\n        </div>\n        <div\n          style=\"\n            border: 1px solid #ffc000;\n            padding: 10px;\n            display: flex;\n            justify-content: space-between;\n          \"\n        >\n          {% if doc.contact_person %}\n              {% set contact = frappe.get_doc(\"Contact\", doc.contact_person) %}\n              <div>\n                <h3 style=\"font-weight: bold\">Email:\n                \n                  {{ (contact.email_ids | selectattr(\"is_primary\") |\n                  map(attribute=\"email_id\") | first) if contact.email_ids else \"\" }}\n                  </h3>\n                </div>\n          {% else %}\n              <div>\n                <h3 style=\"font-weight: bold\">Email:</h3>\n                <span></span>\n              </div>\n          {% endif %}\n          <h3 style=\"font-weight: bold\"></h3>\n        </div>\n        <div\n          style=\"\n            border: 1px solid #ffc000;\n            background-color: #ffc000;\n            padding: 10px;\n            display: flex;\n            justify-content: space-between;\n          \"\n        >\n          <div>\n            <h3 style=\"font-weight: bold\">Offer For Vehicle:</h3>\n            <span></span>\n          </div>\n          <h3 style=\"font-weight: bold\"></h3>\n        </div>\n        <div\n          style=\"\n            border: 1px solid #ffc000;\n            padding: 10px;\n            display: flex;\n            justify-content: space-between;\n          \"\n        >\n          <div>\n            <h3 style=\"font-weight: bold\">Number of Vehicle: {{doc.custom_number_of_cars}}</h3>\n            <span></span>\n          </div>\n          <h3 style=\"font-weight: bold\"></h3>\n        </div>\n        <div\n          style=\"\n            border: 1px solid #ffc000;\n            background-color: #ffc000;\n            padding: 10px;\n            display: flex;\n            justify-content: space-between;\n          \"\n        >\n        {% set emp = frappe.db.get_value(\"Employee\", {\"user_id\": doc.owner}, [\"employee_name\", \"cell_number\"], as_dict=True) %}\n          <div>\n            <h3 style=\"font-weight: bold\">Sales Consultant: {{ emp.employee_name or \"\" }}</h3>\n            <span></span>\n          </div>\n          <h3 style=\"font-weight: bold\"></h3>\n        </div>\n        <div\n          style=\"\n            border: 1px solid #ffc000;\n            padding: 10px;\n            display: flex;\n            justify-content: space-between;\n          \"\n        >\n          <div>\n            <h3 style=\"font-weight: bold\">Mobile Number: {{ emp.cell_number or \"\" }}</h3>\n            <span></span>\n          </div>\n          <h3 style=\"font-weight: bold\"></h3>\n        </div>\n        <div\n          style=\"\n            border: 1px solid #ffc000;\n            background-color: #ffc000;\n            padding: 10px;\n            display: flex;\n            justify-content: space-between;\n          \"\n        >\n          <div>\n            <h3 style=\"font-weight: bold\">Date: {{doc.transaction_date}}</h3>\n          </div>\n          <h3 style=\"font-weight: bold\"></h3>\n        </div>\n      </section>\n      <div class=\"page-break\"></div>\n      <section>\n        <img src=\"/assets/tst/images/quotation_intro.jpeg\" width=\"100%\" />\n      </section>\n      <div class=\"page-break\"></div>\n      <section>\n        <img src=\"/assets/tst/images/quotation_why.jpeg\" width=\"100%\" />\n      </section>\n      <div class=\"page-break\"></div>\n      <section>\n        <img src=\"/assets/tst/images/quotation_services.png\" width=\"100%\" />\n      </section>\n      <div class=\"page-break\"></div>\n      <section>\n        <img src=\"/assets/tst/images/quotation_benefits.jpeg\" width=\"100%\" />\n      </section>\n      <div class=\"page-break\"></div>\n      <section>\n        <img src=\"/assets/tst/images/services_5.jpeg\" width=\"100%\" />\n      </section>\n      <div class=\"page-break\"></div>\n      <section>\n        <img\n          src=\"/assets/tst/images/quotation_financial_customers.jpeg\"\n          width=\"100%\"\n        />\n      </section>\n      <div class=\"page-break\"></div>\n      <!-- Offer Table -->\n  <section dir=\"rtl\">\n    <img src=\"/assets/tst/images/offer.png\" width=\"150\" class=\"mb-3\" />\n    <table class=\"table table-bordered text-center align-middle\" style=\"border-color:#000; font-size:18px;\">\n      <thead style=\"background-color:#ffc000; color:#000;\">\n        <tr>\n          <th style=\"width: 20%;\">\u0627\u0644\u0628\u0646\u062f</th>\n          <th style=\"width: 30%;\">\u0627\u0644\u0628\u064a\u0627\u0646</th>\n          <th style=\"width: 15%;\">\u062a\u0643\u0644\u0641\u0629 \u0627\u0644\u0648\u062d\u062f\u0629</th>\n          <th style=\"width: 15%;\">\u0639\u062f\u062f \u0627\u0644\u0627\u062c\u0647\u0632\u0629</th>\n          <th style=\"width: 20%;\">\u0627\u0644\u0645\u0633\u062a\u062d\u0642 \u0639\u0646\u062f \u0627\u0644\u0639\u0627\u0642\u062f</th>\n        </tr>\n      </thead>\n      <tbody style=\"font-size:13px;\">\n  {% set subscription_groups = {} %}\n  {% set normal_items = [] %}\n  {% for item in doc.items %}\n    {% if item.custom_subscription == 1 %}\n      {% set _ = subscription_groups.setdefault(item.custom_subscription_data, []).append(item) %}\n    {% else %}\n      {% set _ = normal_items.append(item) %}\n    {% endif %}\n  {% endfor %}\n\n  {% for key, items in subscription_groups.items() %}\n    {% set total_amount = items | map(attribute='amount') | sum %}\n    {% set total_qty = items | map(attribute='qty') | sum %}\n    {% set total_rate = items | map(attribute='rate') | sum %}\n    {% set avg_qty = (total_qty / (items | length)) | round(2) %}\n    {% set avg_rate = (total_rate) | round(2) %}\n    <tr style=\"background-color:#fff9e6;\">\n      <td>{{ key }}</td>\n      <td>{{ key }}</td>\n      <td class=\"text-end\">{{ frappe.utils.fmt_money(avg_rate, currency=doc.currency) }}</td>\n      <td>{{ avg_qty }}</td>\n      <td class=\"text-end\">{{ frappe.utils.fmt_money(total_amount, currency=doc.currency) }}</td>\n    </tr>\n  {% endfor %}\n\n  {% for item in normal_items %}\n    <tr>\n      <td>{{ item.item_name }}</td>\n      <td style=\"white-space: pre-wrap;\">{{ item.description or \"\" }}</td>\n      <td class=\"text-end\">{{ frappe.utils.fmt_money(item.rate or 0, currency=doc.currency) }}</td>\n      <td>{{ item.qty }}</td>\n      <td class=\"text-end\">{{ frappe.utils.fmt_money(item.amount or 0, currency=doc.currency) }}</td>\n    </tr>\n  {% endfor %}\n\n  <!-- Totals -->\n  <tr style=\"background-color:#f7f7f7; font-weight:bold;\">\n    <td colspan=\"2\">\u0627\u0644\u0645\u062c\u0645\u0648\u0639</td>\n    <td colspan=\"2\" rowspan=\"3\" class=\"text-center align-middle\">{{ doc.in_words or \"\" }}</td>\n    <td style=\"background-color:#ffb719\" class=\"text-end\">{{ frappe.utils.fmt_money(doc.total or 0, currency=doc.currency) }}</td>\n  </tr>\n  <tr style=\"background-color:#f7f7f7; font-weight:bold;\">\n    <td colspan=\"2\">\u0627\u0644\u0636\u0631\u064a\u0628\u0629 15%</td>\n    <td style=\"background-color:#ffb719\" class=\"text-end\">{{ frappe.utils.fmt_money(doc.total_taxes_and_charges or 0, currency=doc.currency) }}</td>\n  </tr>\n  <tr style=\"background-color:#ffe299; font-weight:bold;\">\n    <td colspan=\"2\">\u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a \u0634\u0627\u0645\u0644 \u0627\u0644\u0636\u0631\u064a\u0628\u0629</td>\n    <td style=\"background-color:#ffb719\" class=\"text-end\">{{ frappe.utils.fmt_money(doc.grand_total or 0, currency=doc.currency) }}</td>\n  </tr>\n</tbody>\n\n    </table>\n  </section>\n      <div class=\"page-break\"></div>\n      <section>\n        <img src=\"/assets/tst/images/terms_and_conditions.jpeg\" width=\"100%\" />\n      </section>\n    </main>\n  </body>\n</html>\n",
